<Keyboard>
	<HideFromKeyboardMenu>True</HideFromKeyboardMenu>
	<BorderThickness>1,30,1,1</BorderThickness>
	<Grid>
		<Rows>1</Rows>
		<Cols>5</Cols>
	</Grid>
	<Keys>
		<DynamicKey Col="0" Height="1" Row="0" Width="1">
			<Label>Ride/\nDismount</Label>
			<Index>0</Index>
			<Text>c</Text>
			</DynamicKey>
		<DynamicKey Col="1" Height="1" Row="0" Width="1">
			<Label>Auto\nPillar</Label>
			<Index>1</Index>
			<Symbol>MinecraftPlaceBlock</Symbol>
			<Text>l</Text>
			</DynamicKey>
		<DynamicKey Col="2" Height="1" Row="0" Width="1">
			<Label>Night\nVision</Label>
			<Index>2</Index>
			<Symbol>MinecraftNightVision</Symbol>
			<Action>F12</Action>
			</DynamicKey>
        <DynamicKey Col="3" Height="1" Row="0" Width="1">
			<Label>Swap mine\nand place</Label>
			<Index>3</Index>
			<Symbol>MinecraftSwapMinePlace</Symbol>
			<Action>F10</Action>
			</DynamicKey>
		<DynamicKey Col="4" Height="1" Row="0" Width="1">
			<Label>Back</Label>
			<Index>4</Index>
			<Symbol>BackIcon</Symbol>
			<Action>BackFromKeyboard</Action>
			</DynamicKey>
	</Keys>
</Keyboard>